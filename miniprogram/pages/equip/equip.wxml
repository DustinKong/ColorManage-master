<cu-custom bgColor="bg-blue" isBack="{{true}}">
	<view slot="backText">返回</view>
	<view slot="content">我的配方</view>
</cu-custom>
<view wx:for="{{info}}" class="flex padding">
	<image style="background-color:{{item.mycolor[0].colornum}}; height:100rpx;width:100rpx; top:10rpx;" class="padding"></image>
	<view class="desc name padding">
		<view class="text-center name text-bold">名字：{{item.mycolor[0].name}}</view>
		<view class="text-center name text-bold">创建时间：{{item.mycolor[0].time}}</view>
	</view>
</view>